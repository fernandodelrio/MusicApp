<?xml version='1.0' encoding='utf-8'?>
<widget id="com.delrio.musicapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>MusicApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
	<feature name="Youtube">
        <param name="android-package" value="com.delrio.musicapp.plugins.Youtube" />
    </feature>
</widget>
